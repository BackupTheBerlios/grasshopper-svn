AVR32_PATCH_SITE=http://avr32linux.org/twiki/pub/Main

AVR32_LINUX_PATCH=LinuxPatches/linux-$(LINUX_HEADERS_VERSION)-avr1.patch.bz2
AVR32_UBOOT_PATCH=UbootPatches/u-boot-$(UBOOT_VERSION)-avr1.patch.bz2
# AVR32_UCLIBC_PATCH=MicroClibcPatches/uClibc-$(UCLIBC_VER)-avr1.patch.bz2
AVR32_UCLIBC_PATCH=MicroClibcPatches/uClibc-$(UCLIBC_VER)-avr1.patch
AVR32_GCC_PATCH=GccPatches/gcc-$(GCC_VERSION)-atmel.0.99.2.patch.bz2
# AVR32_GCC_PATCH=GccPatches/gcc-$(GCC_VERSION)-atmel.0.99.2.patch

AVR32_PATCH_DIR=target/device/Atmel/AVR32

AVR32_PATCHES=$(AVR32_LINUX_PATCH) $(AVR32_UBOOT_PATCH) $(AVR32_UCLIBC_PATCH) $(AVR32_GCC_PATCH)

UBOOT_PATCH_DIR=$(dir $(AVR32_PATCH_DIR)/$(AVR32_UBOOT_PATCH))
LINUX26_PATCH_DIR=$(dir $(AVR32_PATCH_DIR)/$(AVR32_LINUX_PATCH))
UCLIBC_PATCH_DIR=$(dir $(AVR32_PATCH_DIR)/$(AVR32_UCLIBC_PATCH))
BR2_BOARD_PATCH=$(AVR32_PATCH_DIR)

LINUX26_FORMAT = uImage

ifeq ($(BR2_BINUTILS_VERSION),"2.16.1-atmel.0.99.2")
BINUTILS_SITE=$(AVR32_PATCH_SITE)/BinutilsPatches
BINUTILS_NO_MPFR:=y
endif

TARGETS := avr32 $(TARGETS)

.PHONY: avr32

